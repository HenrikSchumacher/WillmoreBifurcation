Cell[BoxData[
 RowBox[{"libfun", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"Real", ",", "3"}], "}"}], ",", 
   "\"\<clipGenerializedEigenvalues\>\"", ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"\"\<Ain\>\"", ",", 
       RowBox[{"{", 
        RowBox[{"Real", ",", "3"}], "}"}], ",", "\"\<Constant\>\""}], "}"}], 
     ",", 
     RowBox[{"{", 
      RowBox[{"\"\<Bin\>\"", ",", 
       RowBox[{"{", 
        RowBox[{"Real", ",", "3"}], "}"}], ",", "\"\<Constant\>\""}], "}"}]}],
     "}"}], ",", 
   RowBox[{"{", "\[IndentingNewLine]", 
    RowBox[{
    "\"\<\nmint n = Ain.dimensions()[0];\nmint k = \
std::min(Ain.dimensions()[1],Ain.dimensions()[2]);\nmint k2 = k*k;\nmint i = \
0;\nmma::TensorRef<mreal> Cout = mma::makeTensor<mreal>({n, k, k});\n\nmreal \
*ain = Ain.begin();\nmreal *bin = Bin.begin();\nmreal *cout = Cout.begin();\n\
\nEigen::GeneralizedSelfAdjointEigenSolver<Eigen::MatrixXd> eigs(k);\n\
Eigen::Map<Eigen::MatrixXd> A(NULL,k,k);\nEigen::Map<Eigen::MatrixXd> \
B(NULL,k,k);\nEigen::Map<Eigen::MatrixXd> C(NULL,k,k);\n\n#pragma omp \
parallel for shared(ain, bin, cout) private(i) firstprivate(A,B,C,k2,k,eigs) \
num_threads(omp_get_num_procs())\nfor (i = 0; i < n; i++)\n{\n\tnew (&A) \
Eigen::Map<Eigen::MatrixXd>(&ain[i * k2], k, k);\n\tnew (&B) \
Eigen::Map<Eigen::MatrixXd>(&bin[i * k2], k, k);\n\tnew (&C) \
Eigen::Map<Eigen::MatrixXd>(&cout[i * k2], k, k);\n\teigs.compute(A,B);\n\tC \
= B * eigs.eigenvectors() * eigs.eigenvalues().cwiseMax(0).asDiagonal() * \
eigs.eigenvectors().inverse();\n}\n\>\"", ",", "\[IndentingNewLine]", 
     RowBox[{"CReturn", "[", "\"\<Cout\>\"", "]"}]}], "\[IndentingNewLine]", 
    "}"}]}], "]"}]], "Input",
